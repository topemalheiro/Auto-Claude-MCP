{
  "name": "@auto-claude/types",
  "version": "2.7.7",
  "type": "module",
  "description": "Shared TypeScript type definitions for Auto Claude",
  "homepage": "https://github.com/AndyMik90/Auto-Claude",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndyMik90/Auto-Claude.git",
    "directory": "packages/types"
  },
  "author": {
    "name": "Auto Claude Team",
    "email": "119136210+AndyMik90@users.noreply.github.com"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
